apiVersion: jaegertracing.io/v1
kind: Jaeger
metadata:
  name: {{ .Env.JAEGER_NAME }}
spec:
  strategy: production
  storage:
    type: elasticsearch
    elasticsearch:
      nodeCount: {{ .Env.ELASTICSEARCH_NODECOUNT }}
      resources:
        requests:
          cpu: 200m
          memory: 1Gi
        limits:
          memory: 1Gi
